{   "requirements": ["build_steps"],
    "base_image": "alpine:latest",
    "build_steps": [
        "FROM alpine:latest AS base",
        "WORKDIR /usr/src/app",
        "COPY {{ aplinejs_app_dir }}/ .",
        ""
    ],
    "output_directory": "/usr/src/app/generics",
    "build_stage_name": "aplinejs-build",
    "defaults": {
        "aplinejs_app_dir": "generics",
        "aplinejs_output_dir": "MyAlpineJsApp"
    }
}
